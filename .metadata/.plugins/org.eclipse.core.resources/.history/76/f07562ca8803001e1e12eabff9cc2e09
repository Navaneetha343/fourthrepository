package package1;
import java.util.Arreys;
public class AddElement {
public static void main(String[] args) {
	
	int[] arr= {1,2,2,3,4,5};
	int[] temparr=new int[arr.length+1];
	temparr=arr;
	int add =6;
	for(int i=0; i<arr.length;i++) {
		temparr[i]=arr[i];
	}
	 temparr[arr.length] = add;
	 for(int j=0; j<temparr.length;j++) {
		 System.out.println(temparr[j]);
	 }
	// temparr= addElement(arr, add);
	//temparr[temparr.length-1]= add;
//System.out.println( Arreys.toString(temparr))
}
}
